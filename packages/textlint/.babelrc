{
  "presets": [
    "latest"
  ],
  "plugins": [
    "transform-inline-imports-commonjs",
    "add-module-exports"
  ],
  "env": {
    "development": {
      "plugins": [
        "jsdoc-to-assert",
        [
          "babel-plugin-espower",
          {
            "embedAst": true
          }
        ]
      ]
    }
  }
}
